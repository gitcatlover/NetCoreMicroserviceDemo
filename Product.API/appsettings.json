{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConsulSetting": {
    "ServiceName": "ProductService",
    "ServiceIP": "localhost",
    "ServiceHealthCheck": "/api/healthcheck/get",
    "ConsulAddress": "http://host.docker.internal:8500" //注意docker容器内部无法使用localhost范文宿主机器，如果是控制台启动就用localhost
  }
}
